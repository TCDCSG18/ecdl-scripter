<div class="cent-pan center">
  <h2><%= I18n.t('submit_token_title', {:scope => 'devise'}) %></h2>
  <%= form_for(resource, :as => resource_name, :url => [resource_name, :checkga], :html => { :method => :put }) do |f| %>
    <%= f.hidden_field :tmpid, {:value => @tmpid} %>
    <p><%= f.number_field :gauth_token, :autocomplete => :off, :class => "form-control"%></p>
    <p><%= f.submit I18n.t('submit_token', {:scope => 'devise'}), :class => 'btn btn-success' %></p>
  <% end %>
</div>
